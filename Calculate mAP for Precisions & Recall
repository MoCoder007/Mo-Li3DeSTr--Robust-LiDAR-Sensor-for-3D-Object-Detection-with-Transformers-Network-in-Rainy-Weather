import numpy as np

precisions = np.array([0.9, 0.85, 0.83, 0.80, 0.75, 0.70, 0.65, 0.60, 0.55, 0.50])
recalls = np.linspace(0, 1, len(precisions))  # Assuming uniform recall values

# Calculate the area under the precision-recall curve (AP)
ap = np.trapz(precisions, recalls)

# Calculate the mean average precision (mAP)
mAP = ap / len(precisions)

mAP
